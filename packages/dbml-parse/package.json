{
  "$schema": "https://json.schemastore.org/package",
  "name": "@dbml/parse",
  "version": "6.1.0",
  "description": "DBML parser v2",
  "author": "Holistics <dev@holistics.io>",
  "license": "Apache-2.0",
  "homepage": "https://dbml.dbdiagram.io",
  "repository": "https://github.com/holistics/dbml/tree/master/packages/dbml-parse",
  "keywords": [
    "dbml",
    "dbml-parse"
  ],
  "main": "./dist/dbml-parse.js",
  "module": "./dist/dbml-parse.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/dbml-parse.mjs",
      "require": "./dist/dbml-parse.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc --noEmit && vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "prepublish": "npm run build",
    "prepare": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "devDependencies": {
    "monaco-editor-core": "^0.44.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  },
  "engines": {
    "node": ">=18"
  }
}
