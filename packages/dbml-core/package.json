{
  "$schema": "https://json.schemastore.org/package",
  "name": "@dbml/core",
  "version": "6.1.0",
  "description": "> TODO: description",
  "author": "Holistics <dev@holistics.io>",
  "license": "Apache-2.0",
  "homepage": "https://dbml.dbdiagram.io",
  "repository": "https://github.com/holistics/dbml/tree/master/packages/dbml-core",
  "keywords": [
    "dbml",
    "dbml-core"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "types"
  ],
  "types": "./types/index.d.ts",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest tests",
    "coverage": "vitest run --coverage",
    "build:parser": "npx babel-node src/parse/deprecated/buildParser.js",
    "build:lib": "tsc --noEmit && vite build",
    "build": "npm run build:lib && npm run build:parser",
    "prepublish": "npm run build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "@dbml/parse": "^6.1.0",
    "antlr4": "^4.13.1",
    "lodash": "^4.17.15",
    "parsimmon": "^1.13.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "bluebird": "^3.5.5",
    "vite-plugin-commonjs": "^0.10.4"
  },
  "gitHead": "5cb80e1aa38fb9a4dbe3079e39c9ef93cd4dc556",
  "engines": {
    "node": ">=16"
  }
