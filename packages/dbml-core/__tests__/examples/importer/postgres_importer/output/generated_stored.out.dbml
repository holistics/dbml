Table "users" {
  "identity_data" jsonb
  "email" text
}

Table "json_operators" {
  "id" serial [pk, increment]
  "data" jsonb
  "field_text" text
  "field_json" jsonb
  "nested_text" text
  "nested_json" jsonb
}

Table "other_operators" {
  "id" serial [pk, increment]
  "a" int
  "b" int
  "left_shift" int
  "right_shift" int
  "gte" boolean
  "lte" boolean
  "neq" boolean
  "as_text" text
}
