Function public.increment {
  returns integer
  args [len_from: integer, len_to: integer]
  body `
      DECLARE
          film_count INTEGER;
      BEGIN
          SELECT COUNT(*)
          INTO film_count
          FROM film
          WHERE length BETWEEN len_from AND len_to;
          RETURN film_count;
      END;
  `
  language plpgsql
  behavior volatile
  security invoker
}

Function simple_add {
  schema public
  returns integer
  args [a: integer, b: integer]
  body `
      BEGIN
        RETURN a + b;
      END;
  `
  language plpgsql
  behavior immutable
  security definer
}
