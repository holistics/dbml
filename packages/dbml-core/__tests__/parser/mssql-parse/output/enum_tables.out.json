{
  "tables": [
    {
      "name": "jobs",
      "fields": [
        {
          "name": "id",
          "type": {
            "type_name": "integer"
          },
          "pk": true
        },
        {
          "name": "status",
          "type": {
            "type_name": "nvarchar(255)"
          },
          "not_null": true,
          "checks": [
            {
              "expression": "[status] IN ('created', 'running', 'done', 'failed', 'wait for validation')"
            }
          ]
        }
      ]
    },
    {
      "name": "orders",
      "fields": [
        {
          "name": "id",
          "type": {
            "type_name": "int"
          },
          "pk": true
        },
        {
          "name": "created_at",
          "type": {
            "type_name": "varchar(255)"
          }
        },
        {
          "name": "priority",
          "type": {
            "type_name": "nvarchar(255)"
          },
          "not_null": true,
          "checks": [
            {
              "expression": "[priority] IN ('low', 'medium', 'high')"
            }
          ]
        },
        {
          "name": "status",
          "type": {
            "type_name": "nvarchar(255)"
          },
          "not_null": true,
          "checks": [
            {
              "expression": "[status] IN ('pending', 'processing', 'done')"
            }
          ]
        }
      ]
    }
  ]
}
